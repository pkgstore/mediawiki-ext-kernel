{
  "name": "pkgstore/mediawiki-ext-kernel",
  "type": "mediawiki-extension",
  "description": "MediaWiki CMF Store kernel extension.",
  "license": "MIT",
  "homepage": "https://ihub.to/",
  "keywords": [
    "mediawiki",
    "kernel"
  ],
  "authors": [
    {
      "name": "Kai Kimera",
      "email": "mail@kai.kim",
      "homepage": "https://kai.kim/",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/pkgstore/mediawiki-ext-kernel/issues",
    "source": "https://github.com/pkgstore/mediawiki-ext-kernel"
  },
  "require": {
    "composer/installers": "^1.0.1",
    "ext-json": "*",
    "ext-xml": "*",
    "ext-yaml": "*",
    "php": ">=8.2"
  },
  "autoload": {
    "classmap": [
      "MW_EXT_Kernel.class.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "prepend-autoloader": false
  },
  "extra": {
    "installer-name": "MW_EXT_Kernel"
  }
}
